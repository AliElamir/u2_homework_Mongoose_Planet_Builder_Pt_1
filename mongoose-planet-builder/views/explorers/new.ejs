<%- include('../partials/header') %>

<div>Please enter the planet explorer</div>
<section id="badge-container">
  <% explorers.forEach((e) => { %>
    <article class="badge"><%= e.name %></article>
  <% }) %>
</section>

<form id="add-performer-form" action="/explorers" method="POST">
  <label>Name:</label>
  <input type="text" name="name">
  <input type="submit" value="Add Explorer">
</form>


<%- include('../partials/footer') %>
